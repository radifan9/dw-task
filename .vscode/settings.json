{
  "css.enabledLanguages": ["html", "hbs"],
  "css.styleSheets": [
    "node_modules/bootstrap/dist/css/bootstrap.css",
    "src/**/*.css",
    "src/**/*.scss"
  ],
  "files.associations": {
    "*.hbs": "html"
  },
  "emmet.includeLanguages": {
    "handlebars": "html"
  },

  "[handlebars]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": null
  }
}
